</model> 


<!-- Gear Animation -->

<!--       Left Gear      -->

    <animation>
        <type>rotate</type>
        <object-name>left_gear</object-name>
        <object-name>left_gear2</object-name>
        <property>gear/gear[1]/position-norm</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>95</dep>
            </entry>
            <entry>
                <ind>1.0</ind>
                <dep>0</dep>
            </entry>
        </interpolation>
        <center>
            <x-m>6.407</x-m>
            <y-m>-4.300</y-m>
            <z-m>-3.450</z-m>
        </center>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

<!-- Right Gear -->
    <animation>
        <type>rotate</type>
        <object-name>right_gear</object-name>
        <object-name>right_gear2</object-name>
        <property>gear/gear[2]/position-norm</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>95</dep>
            </entry>
            <entry>
                <ind>1.0</ind>
                <dep>0</dep>
            </entry>
        </interpolation>
        <center>
            <x-m>6.407</x-m>
            <y-m>4.300</y-m>
            <z-m>-3.450</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

<!-- Front Gear -->
    <animation>
        <type>rotate</type>
        <object-name>nose_gear</object-name>
        <property>gear/gear[0]/position-norm</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>120</dep>
            </entry>
            <entry>
                <ind>0.2</ind>
                <dep>115</dep>
            </entry>
            <entry>
                <ind>1.0</ind>
                <dep>0</dep>
            </entry>
        </interpolation>
        <center>
            <x-m>-22.500</x-m>
            <y-m>0.0</y-m>
            <z-m>-3.550</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

<!-- Gear disapears when raised -->

<animation>
        <type>select</type>
        <object-name>right_gear</object-name>
        <object-name>right_gear2</object-name>
        <condition>
        <property>gear/gear[0]/position-norm</property>
        </condition>
    </animation>

<animation>
        <type>select</type>
        <object-name>nose_gear</object-name>
        <condition>
        <property>gear/gear[0]/position-norm</property>
        </condition>
    </animation>

<animation>
